<p id=”alert”><%= alert %></p>

<h1>Login</h1>

<%= form_tag '/login' do %>

    <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag("email", nil)%>
    </div>
    <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag("password", nil)%>
    </div>
    <%= submit_tag "Submit" %>

<% end %>